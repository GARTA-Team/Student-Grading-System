      "import/no-unresolved": 0,
      "react/state-in-constructor": 0,
      "react/prop-types": 0,
      "react/no-did-mount-set-state": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
